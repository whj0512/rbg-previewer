{
  "name": "rbgpreviewer",
  "displayName": "RbgPreviewer",
  "description": "Preview .rbg graph files in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onLanguage:rbg"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [{
      "id": "rbg",
      "extensions": [".rbg"],
      "aliases": ["RBG Graph"]
    }],
    "commands": [
      {
        "command": "rbgpreviewer.preview",
        "title": "Preview RBG Graph",
        "icon": "$(preview)"
      }
    ],
    "menus": {
      "editor/title": [{
        "when": "resourceExtname == .rbg",
        "command": "rbgpreviewer.preview",
        "group": "navigation"
      }],
      "explorer/context": [{
        "when": "resourceExtname == .rbg",
        "command": "rbgpreviewer.preview",
        "group": "navigation"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
